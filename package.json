    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "jstoxml": "^2.0.3",
    "lodash": "^4.17.20",
